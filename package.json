{
  "name": "aurelia-cached-getter",
  "version": "0.1.0",
  "description": "Cache your Aurelia GET requests into localstorage for offline access",
  "keywords": [
    "aurelia",
    "plugin",
    "http",
    "offline"
  ],
  "homepage": "http://futureground.net",
  "bugs": {
    "url": "https://github.com/jbellsey/aurelia-cached-getter"
  },
  "license": "MIT",
  "author": "Jeff Bellsey <jbellsey@gmail.com>",
  "main": "dist/commonjs/http-cached-getter.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbellsey/aurelia-cached-getter"
  },
  "jspm": {
    "main": "http-cached-getter",
    "format": "amd",
    "directories": {
      "lib": "dist/amd"
    },
    "dependencies": {
      "aurelia/http-client": "github:aurelia/http-client@^0.10.2"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.1.13",
